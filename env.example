# StoreHub Knowledge Base Auditor - Environment Configuration

# Server Configuration
PORT=3001
NODE_ENV=development

# Logging Configuration
LOG_LEVEL=debug
LOG_FILE=./logs/app.log

# External API Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_ENDPOINT=https://generativelanguage.googleapis.com/v1beta/models/gemini-1.5-flash:generateContent

# Rules Engine Configuration
DEFAULT_OUTDATED_THRESHOLD_MONTHS=6
MAX_CONCURRENT_RULES=5

# Security Configuration
CORS_ORIGIN=http://localhost:3000
API_RATE_LIMIT_WINDOW_MS=900000
API_RATE_LIMIT_MAX_REQUESTS=100 